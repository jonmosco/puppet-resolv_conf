{
  "name": "jonnyx-resolv_conf",
  "version": "1.0.5",
  "author": "Jon Mosco",
  "summary": "Manage /etc/resolv.conf on Linux Servers",
  "license": "Apache-2.0",
  "source": "https://github.com/jonmosco/puppet-resolv_conf",
  "project_page": "https://github.com/jonmosco/puppet-resolv_conf",
